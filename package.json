{
    "name": "@fdmg/css-grid",
    "version": "2.0.7",
    "description": "FDMG CSS Grid with Flexbox fallback",
    "style": "dist/css/grid.css",
    "scripts": {
        "build": "next build",
        "build:ci": "npm run webpack:prod && npm run release:grid && npm run release:flex && npm run copy:dts",
        "copy:dts": "npx copyfiles -V \"./dist.d.ts/components/GridContainer.d.ts\" \"./dist\"",
        "dev": "next dev",
        "json-to-scss": "json-to-scss public/assets/design-tokens/* src/design-tokens",
        "release:grid": "sass --no-source-map --style=compressed scss/grid.scss dist/css/grid.css",
        "release:flex": "sass --no-source-map --style=compressed scss/flex.scss dist/css/flex.css",
        "start": "next start",
        "test": "echo \"No tests specified\" && exit 0",
        "webpack:prod": "npx cross-env NODE_ENV=production webpack --mode=production"
    },
    "author": "Willem Liu",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git@github.com:FDMediagroep/fdmg-css-grid.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@fdmg/design-system": "^0.4.42",
        "@types/fetch-jsonp": "^1.0.0",
        "@types/micro-cors": "^0.1.1",
        "@types/node": "^14.14.37",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@typescript-eslint/typescript-estree": "^4.21.0",
        "@webcomponents/webcomponentsjs": "^2.5.0",
        "autoprefixer": "^10.2.5",
        "copy-webpack-plugin": "^8.1.1",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.4",
        "date-input-polyfill": "^2.14.0",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.23.2",
        "fetch-jsonp": "^1.1.3",
        "js-cookie": "^2.2.1",
        "json-to-scss": "^1.6.2",
        "micro-cors": "^0.1.1",
        "mini-css-extract-plugin": "^1.4.1",
        "next": "^10.1.3",
        "nookies": "^2.5.2",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^5.2.0",
        "postcss-normalize": "^9.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.2",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "ts-loader": "^8.1.0",
        "typescript": "^4.2.4",
        "webpack-cli": "^4.6.0",
        "xmldom": "^0.5.0"
    }
}
