{
    "name": "@fdmg/css-grid",
    "version": "2.0.0",
    "description": "FDMG CSS Grid with Flexbox fallback",
    "style": "dist/css/grid.css",
    "scripts": {
        "build": "next build",
        "build:ci": "npm run release:grid && npm run release:flex",
        "dev": "next dev",
        "release:grid": "sass --no-source-map --style=compressed scss/grid.scss dist/css/grid.css",
        "release:flex": "sass --no-source-map --style=compressed scss/flex.scss dist/css/flex.css",
        "start": "next start",
        "test": "echo \"No tests specified\" && exit 0"
    },
    "author": "Willem Liu",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git@github.com:FDMediagroep/fdmg-css-grid.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/node": "^14.11.10",
        "@types/react": "^16.9.4",
        "@types/react-dom": "^16.9.1",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "@typescript-eslint/typescript-estree": "^4.4.1",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "next": "^9.5.5",
        "prettier": "^2.1.2",
        "react": "^16.14.0",
        "react-dom": "^16.10.1",
        "sass": "^1.27.0",
        "typescript": "^3.6.3",
        "webpack-cli": "^3.3.9"
    }
}
